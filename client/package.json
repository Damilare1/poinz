{
  "name": "poinz-client",
  "version": "0.5.2",
  "description": "Distributed Planning Poker",
  "private": true,
  "author": "xeronimus@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Zuehlke/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuehlke/poinz.git"
  },
  "scripts": {
    "start": "npm run serve",
    "lint": "eslint \"app/**/*.js\" \"test/**/*.js\" \"cypress/**/*.js\"",
    "format": "prettier --write \"app/**/*.js\" \"cypress/**/*.js\" \"test/**/*.js\"",
    "test": "jest",
    "test:e2e": "npm run cypress:run",
    "pretest": "npm run lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "serve": "BABEL_ENV=development webpack-dev-server --progress --colors --port 9000",
    "build": "BABEL_ENV=production webpack --progress --colors --bail --json --config  webpack.production.config.js > webpack-build-stats.json"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/**/*Test.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.10.5",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "axios": "0.20.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "blueimp-md5": "2.17.0",
    "css-loader": "4.2.2",
    "cypress": "^5.0.0",
    "date-fns": "^2.16.0",
    "eslint": "7.7.0",
    "eslint-plugin-react": "7.20.6",
    "file-loader": "6.0.0",
    "font-awesome": "4.7.0",
    "history": "5.0.0",
    "jest": "^26.4.2",
    "loglevel": "1.7.0",
    "node-polyglot": "^2.4.0",
    "purecss": "2.0.3",
    "react": "16.13.1",
    "react-anchorify-text": "2.4.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^11.0.3",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "socket.io-client": "2.3.0",
    "style-loader": "1.2.1",
    "styled-components": "^5.1.1",
    "url-loader": "4.1.0",
    "uuid": "8.3.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "engines": {
    "node": ">=9"
  }
}
