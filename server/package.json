{
  "name": "poinz-server",
  "version": "0.8.5",
  "description": "Distributed Planning Poker",
  "private": true,
  "author": "xeronimus@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Zuehlke/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuehlke/poinz.git"
  },
  "main": "lib/server.js",
  "scripts": {
    "build": "babel src -d lib --source-maps true",
    "test": "jest \"./test/unit\"",
    "test:integration": "jest \"./test/integration\"",
    "test:migration": "jest --runInBand \"./test/migrations\"",
    "docu": "node ./docu/docuGenerator.js",
    "pretest": "npm run lint",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" \"migrations/**/*.js\" \"./*js\"",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"docu/**/*.js\" \"migrations/**/*.js\"  \"./*js\"",
    "start": "node lib/server.js",
    "start:dev": "node src/server.dev.js",
    "migrate:status": "migrate-mongo status",
    "migrate:down": "migrate-mongo down",
    "migrate": "migrate-mongo up"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/*Test.js"
    ]
  },
  "dependencies": {
    "express": "4.17.1",
    "express-sslify": "1.2.0",
    "fastq": "1.9.0",
    "glob": "7.1.6",
    "migrate-mongo": "8.1.4",
    "mongodb": "3.6.2",
    "papaparse": "5.3.0",
    "redis": "3.0.2",
    "socket.io": "2.3.0",
    "tv4": "1.3.0",
    "uuid": "8.3.1",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "babel-jest": "26.6.1",
    "chalk": "4.1.0",
    "ejs": "3.1.5",
    "eslint": "7.12.1",
    "jest": "26.6.1",
    "socket.io-client": "2.3.1"
  },
  "engines": {
    "node": ">=10"
  }
}
